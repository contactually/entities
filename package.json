{
  "name": "redux-capacitor",
  "version": "0.4.1",
  "main": "index.js",
  "author": "austin@contactually.com",
  "license": "MIT",
  "scripts": {
    "build:package": "cp ./package.json ./lib",
    "build:clean": "rm -rf ./lib",
    "build:compile": "babel src/ -d lib/ --ignore *.spec.js",
    "build": "yarn build:clean && yarn build:compile && yarn build:package",
    "test": "node scripts/test.js --env=jsdom",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "flow:check": "./node_modules/.bin/flow check",
    "hook": "bash ./hooks/install",
    "publish:npm": "yarn version && yarn build && cd ./lib && npm publish",
    "publish:local": "yarn build && cd ./lib && yalc publish && yalc update"
  },
  "devDependencies": {
    "@types/lodash.flow": "3.5.3",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-flow-comments": "6.22.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "flow-bin": "0.54.1",
    "jest": "21.2.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-test-renderer": "16.2.0",
    "webpack": "3.10.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "immutable": "^3.8.1",
    "lodash": "^4.17.2",
    "normalizr": "^3.2.4",
    "prop-types": "^15.6.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.6",
    "redux": "^3.6.0",
    "redux-saga": "0.12.1",
    "reselect": "2.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest/test-setup.js",
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/config/jest/test-setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  }
}
